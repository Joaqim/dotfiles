ewwEdit:
    eww --config /home/jq/dotfiles/home-manager/modules/eww/config/ open centerPanel

ewwLaunch:
    ./home/jq/dotfiles/home-manager/modules/eww/config/scripts/master.sh

hmSwitch:
    home-manager switch --flake ~/dotfiles

nixosSwitch:
    doas nixos-rebuild switch --use-remote-sudo --flake /home/jq/dotfiles

nixosTest:
    doas nixos-rebuild test --use-remote-sudo --flake /home/jq/dotfiles

